def find_median(list1):
    list1.sort()
    length = len(list1)
    if length % 2 == 0:
        median = (list1[length // 2 - 1] + list1[length // 2]) / 2
    else: 
        median = list1[length // 2]
    return median
list1 = [12, 45, 16, 17, 19, 20]
median = find_median(list1)
print('Median is:', median)
